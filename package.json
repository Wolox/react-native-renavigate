{
  "name": "react-native-renavigate",
  "version": "0.2.4",
  "description": "React native navigation made easy using redux",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src && eslint index.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "babel index.js -d lib && babel src -d lib/src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolox/react-native-renavigate.git"
  },
  "keywords": [
    "react-native",
    "react",
    "navigation",
    "router",
    "redux",
    "navigator"
  ],
  "author": "Sebastian Balay <sebastian.balay@wolox.com.ar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolox/react-native-renavigate/issues"
  },
  "homepage": "https://github.com/wolox/react-native-renavigate#readme",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/Examples",
      "/lib"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "./coverage/",
    "testRegex": ".*\\.spec\\.js$"
  },
  "dependencies": {
    "react-native-scrollable-tab-view": "^0.6.0",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-preset-react-native": "^1.9.1",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^18.1.0",
    "react": "^15.4.2",
    "react-native": "^0.40.0"
  },
  "peerDependencies": {
    "react-native": "^0.40.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  }
}
